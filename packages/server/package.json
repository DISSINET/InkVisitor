{
  "name": "inkvisitor-server",
  "version": "1.3.1",
  "engines": {
    "npm": ">=8.19.2",
    "node": ">=16.18.0"
  },
  "scripts": {
    "lint": "tslint --project \"tsconfig.json\"",
    "test": "jest -w 1 -t",
    "start": "nodemon",
    "build": "tsc --build tsconfig.prod.json",
    "start:dist": "node -r module-alias/register ./dist/server/src",
    "jwt": "ts-node -r tsconfig-paths/register ./src/scripts/jwt.ts",
    "swagger": "ts-node -r tsconfig-paths/register ./src/scripts/swagger.ts"
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "../shared"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@shared": "dist/shared",
    "@common": "dist/server/src/common",
    "@modules": "dist/server/src/modules",
    "@service": "dist/server/src/service",
    "@models": "dist/server/src/models",
    "@middlewares": "dist/server/src/middlewares"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "@types/cli-color": "^2.0.2",
    "bcrypt": "^5.0.0",
    "cli-color": "^2.0.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "express-jwt": "8.4.1",
    "helmet": "^3.23.1",
    "http": "0.0.1-security",
    "http-status-codes": "^1.4.0",
    "https": "^1.0.0",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^1.10.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "randomstring": "^1.1.5",
    "rethinkdb-ts": "^2.4.18",
    "socket.io": "^4.6.1",
    "swagger-ui-express": "^4.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.6",
    "@types/email-templates": "^8.0.4",
    "@types/express": "^4.17.6",
    "@types/helmet": "0.0.47",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.13",
    "@types/randomstring": "^1.1.6",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.0.1",
    "eslint": "^8.14.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.6.2",
    "rsync": "^0.6.1",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "ts-jest": "^26.5.1",
    "tslint": "^6.1.2",
    "tslint-lines-between-class-members": "^1.3.6",
    "typescript": "^5.1.3"
  }
}
